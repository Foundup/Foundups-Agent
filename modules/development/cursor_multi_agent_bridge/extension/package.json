{
  "name": "wre-red-dog",
  "displayName": "WRE Red Dog - Autonomous Development",
  "description": "Windsurf Recursive Engine - Autonomous multi-agent development for Cursor IDE",
  "version": "0.1.0",
  "publisher": "foundups",
  "repository": {
    "type": "git",
    "url": "https://github.com/FoundUps/Foundups-Agent"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Extension Packs"
  ],
  "keywords": [
    "wre",
    "autonomous",
    "multi-agent",
    "development",
    "foundups",
    "wsp"
  ],
  "activationEvents": [
    "onCommand:wre.activate",
    "onCommand:wre.openTerminal",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "icon": "assets/red-dog-icon.png",
  "contributes": {
    "commands": [
      {
        "command": "wre.activate",
        "title": "[U+1F415] WRE: Activate Red Dog",
        "category": "WRE"
      },
      {
        "command": "wre.openTerminal",
        "title": "[U+1F415] WRE: Open Terminal",
        "category": "WRE"
      },
      {
        "command": "wre.runAutonomous",
        "title": "[BOT] WRE: Run Autonomous Mode",
        "category": "WRE"
      },
      {
        "command": "wre.checkAgents",
        "title": "[SEARCH] WRE: Check Agent Status",
        "category": "WRE"
      },
      {
        "command": "wre.tokenStatus",
        "title": "ðŸª™ WRE: Token Status",
        "category": "WRE"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "wre.activate",
          "when": "true",
          "group": "navigation@1"
        }
      ],
      "commandPalette": [
        {
          "command": "wre.activate",
          "title": "[U+1F415] Activate WRE Red Dog"
        },
        {
          "command": "wre.openTerminal",
          "title": "[U+1F415] Open WRE Terminal"
        }
      ]
    },
    "keybindings": [
      {
        "command": "wre.activate",
        "key": "ctrl+shift+w",
        "mac": "cmd+shift+w"
      },
      {
        "command": "wre.openTerminal",
        "key": "ctrl+shift+alt+w",
        "mac": "cmd+shift+alt+w"
      }
    ],
    "configuration": {
      "title": "WRE Red Dog",
      "properties": {
        "wre.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Path to Python executable for WRE"
        },
        "wre.projectRoot": {
          "type": "string",
          "default": "",
          "description": "Root path of FoundUps-Agent project"
        },
        "wre.autoStart": {
          "type": "boolean",
          "default": false,
          "description": "Automatically start WRE when opening project"
        },
        "wre.tokenLimit": {
          "type": "number",
          "default": 10000,
          "description": "Default token limit for WRE operations"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}